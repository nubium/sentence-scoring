{
	"name": "nubium/sentence-scoring",
	"description": "Provides tools for badword scoring",
	"license": "proprietary",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Nubium\\SentenceScoring\\": "src/"
		}
	},
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.4.2",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/extension-installer": "^1.0",
		"phpunit/phpunit": "^9.5"
	},
	"autoload-dev": {
		"psr-4": {
			"Nubium\\SentenceScoring\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
